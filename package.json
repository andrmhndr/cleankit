{
  "name": "@andrmhndr/cleankit",
  "description": "A lightweight React utility kit for those who want to be clean.",
  "version": "0.0.33",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.cjs"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "import": "./dist/next/index.mjs",
      "require": "./dist/next/index.cjs"
    }
  },
  "files": [
    "dist",
    "dist/next",
    "dist/react",
    "dist/server"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts"
  },
  "keywords": [
    "react",
    "dialog",
    "provider",
    "utils"
  ],
  "author": "andrmhndr",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8 || ^17 || ^18",
    "react-dom": "^16.8 || ^17 || ^18"
  },
  "dependencies": {},
  "sideEffects": false,
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "next": "^13.5.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.12.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrmhndr/cleankit.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/andrmhndr/cleankit#readme",
  "bugs": {
    "url": "https://github.com/andrmhndr/cleankit/issues"
  }
}
